package ventana;

import javax.swing.JFrame;

public class Ventana2 extends JFrame {
	public Ventana2() {
		setVentana();
		iniciarComponentes();
	}
	private void setVentana() {
		this.setSize(400,400);
		
	}
	public static void main(String[] args) {
		Ventana2 v = new Ventana2();
		v.setVisible(true);

	}

}
